#User input
import numpy as np
while True: 
    try:
        user_input = int(input("Enter number (1 - 50): ")) #int
        if user_input == np.random.randint(1, 50):
            print("Good")
        elif user_input != np.random.randint(1, 50):
            print("no bro")
        break
    except ValueError:
        print("number, dumbass")

    print(np.random.randint(1, 50))
